<body >
<div class="center">
  <table>
    <tr>
      <td><img src="/img/download.png" style="width: 20px; height: 18px;"/></td>
      <td style="padding-right: 10px;"><span>导出</span></td>
      <td><img src="/img/print.png" style="height: 18px;"/></td>
      <td style="padding-right: 350px;"><span>打印</span></td>
      <!-- <td><img src="/img/back.png" style="margin-top:10px;width: 20px; height: 18px;"></td> -->
      <!-- <td><a href="/stockQuery?code=<%=currency%>" style="text-decoration:none;">返回</a></td> -->
    </tr>
  </table>
  <br>
  <br>

  <table class="table" style="width:600px;" border="0"  cellpadding="2" cellspacing="0">
    <caption class="font1" style="background-color: #267097;padding: 5px 0px"><%=currency%> <%=name%></caption>
    <thead>
      <tr >
        <th>姓名</th>
        <th>钱包地址</th>
        <th>持有份数</th>
      </tr>
    </thead>
    <tbody>
    <% result.forEach(function(rs){ %>
    <tr>
      <td align="center" ><%=rs.currency%></td><td><%=rs.counterparty%></td><td><%=rs.value%></td>
    </tr>
    <% })%>
    </tbody>

  </table>
  <div style="height: 20px"></div>
</div>

