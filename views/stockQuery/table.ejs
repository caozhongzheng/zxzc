<title>My Demo 1</title>
<style type="text/css">
  img {
    width:30px;
    height: 29px;
  }
  .down {
    width:100px;
    height:auto;
    float:left;
    display:inline
  }
  .table tr:nth-child(odd) td  {
    background-color: #E2F1EA;

  }
  .font1 {
    font-family: "微软雅黑";
    font-size: 18px;
    color: #fff;
    font-weight: bold;
  }
  .center {
    width: 600px;
    height: 100%;
    background-color: #ffffff;
    padding: 10px 20px;
    margin-top: 20px;
    margin-left: 130px;
  }
  .table > thead > tr > th, .table > thead > tr > td, .table > tbody > tr > td {
    border-top: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
    line-height: 1.42857;
    height:30px;

  }
</style>
  <script>
    var _hmt = _hmt || [];
