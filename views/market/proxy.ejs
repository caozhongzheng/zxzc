<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>我的委托</title>
    <script src="/js/jquery-2.2.3.min.js"></script>
    <script src="/js/jquery-form.js"></script>
    <link href="/css/bootstrap/bootstrap.css" rel="stylesheet">
    <script src="/js/bootstrap/bootstrap.js"></script>

    <script>
        var _hmt = _hmt || [];
        (function() {
            var hm = document.createElement("script");
            hm.src = "//hm.baidu.com/hm.js?423d1e46a3f037d8c42e1b2853f3e69e";
            var s = document.getElementsByTagName("script")[0];
            s.parentNode.insertBefore(hm, s);
        })();
    </script>
    <script type="text/javascript">
        $('#editMsg ').modal('hide').css({
            'margin-top': function () {
                return ($(this).height());
            }
        });
        function tijiaodengdai(obj){
            var offset=$(obj).offset();
           // alert("y="+offset.top);
            $('#myModal ').modal('hide').css({
                'margin-top': function () {
                    return (offset.top-130);
                }
            });

            $('#myModal').modal('show');
            setTimeout(function(){

                $('#imgchange').attr("src","/img/zhengque.png");
                $('#textchange').text("撤单完成");
                $('#tjtext').text("提交成功，我们将在两个工作日内对对该订单进行撤销，请刷新页面进行查看.");
                $('#button').removeAttr("disabled");

            },1000);
        };

        function tijiaodengdai1(obj,sequence){
            $.ajax({
                type: 'POST',
                url:'/market/cancel',
                data: {sequence:sequence},
                dataType: 'json',
                success: function(result){
                    if(result)
                        tijiaodengdai(obj);
                }
            });
        }
    </script>
    <style type="text/css">
        body {
            height: auto;
            width: 100%;
            margin: 0px;
            padding: 0px;
            max-width:970px;
            min-width:753px;
            background-color: #CCE5EC;

        }
        .an1 {
            background-color: #69c9ec;
            height: 43px;
            width: 180px;
            font-family: "微软雅黑";
            font-size: 14px;
            border-radius: 5px;
            border-top-style: none;
            border-right-style: none;
            border-bottom-style: none;
            border-left-style: none;
        }
        .an1:hover{background-color:#91d7f0;
            cursor:pointer;}
        .font4 {
            font-family: "微软雅黑";
            font-size: 14px;
            color: #000;
            font-weight: bold;
        }
        .font2 {
            font-family: "微软雅黑";
            font-size: 13px;
            color: #2d3d5b;
        }
        .font1 {
            font-family: "微软雅黑";
            font-size: 14px;
            color: #fff;
            float: right;
            margin-top: 11px;
            margin-right: 23px;
        }
        .font3 {
            font-family: "微软雅黑";
            font-size: 18px;
            font-weight: bold;
            color: #2d3d5b;
        }
        #div6 {
            float: left;
            font-family: "微软雅黑";
            font-size: 14px;
            color: #666;
            height: auto;
            width: auto;
            font-weight: bold;
            margin-top: -27px;
            margin-left: 135px;
        }
        #div7 {
            width: 100px;
            border: 1px solid #69C9EC;
            height: 80px;
            margin-left: 20px;
            font-family: "微软雅黑";
            font-size: 12px;

            text-align: center;

            margin-top: 20px;
        }
        .font5 {
            font-family: "微软雅黑";
            font-size: 14px;
            font-weight: bold;
            color: #2D3D5B;
        }
        #div8 {
            float: left;
            margin-left: 140px;
            margin-top: -50px;
        }
        #div9 {
            float: right;
            margin-top: -130px;
        }
        #div1 #div2 {
            height: 42px;
            width: auto;
        }
        #div1 {
            width: auto;
            background-color: #ddeef4;

        }
        .div4 {
            height: 185px;
            background-color: #FFFFFF;
            width: auto;
            margin-left: 36px;
            margin-right: 36px;
            box-shadow: 0px 0px 3px lightgray;
            border-radius: 5px 5px 5px 5px;
        }
        .div5 {
            height: 40px;
            width: 120px;
            background-color: #ff9933;
            margin-top: 20px;
        }
    </style>
</head>

<body>
<div id="div1">
    <div id="div2" style="margin-top: 20px"><table width="100%" border="0" cellpadding="0" cellspacing="0">
            <tr>
                <td width="6" bgcolor="#ed7c65">&nbsp;</td>
                <td bgcolor="#c2e1eb" >&nbsp;&nbsp;&nbsp;<span class="font5">我的委托</span></td>
                <td width="52" bgcolor="#c2e1eb"><img src="/img/dj.png" width="51" height="42" /></td>
            </tr>
        </table>
    </div>
    <!--模态框（Modal）-->
    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" data-show="true"
         aria-labelledby="myModalLabel" aria-hidden="false" style=" height: 300px;">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                </div>
                <div class="modal-body">
                    <div>
                        <table width="100%">
                            <tr>
                                <td width="40%" align="right" style="padding-right: 20px">
                                    <img id="imgchange" src="/img/jiazaizhong-063.gif" style="width: 50px;height: 50px" alt=""/>
                                </td>
                                <td id="textchange" width="60%" align="left" class="font3">正在提交...</td>
                            </tr>
                        </table>
                    </div>

                    <div style="padding-left:50px">
                        <p >
                            <span id="tjtext" class="font2">数据正在提交，请耐心等待。</span>
                        </p>
                    </div>
                    <div class="modal-footer">
                        <button id="button" type="button" class="btn btn-primary" data-dismiss="modal" disabled="true">
                            确定
                        </button>

                    </div>
                </div>

            </div><!-- /.modal-content -->
        </div><!-- /.modal -->
    </div>
    <%if (orders.length) {%>
    <% orders.forEach(function (order) {%>

    <div id="div3">
        <% if(order.specification.direction == "sell") {%>
        <div class="div4" id="div4" style="	margin-top: 60px;"><table width="100%"  height="61px" border="0" align="center" cellpadding="0" cellspacing="0"  >
                <tr>
                    <td width="51%" rowspan="2"><div class="div5" id="div5"><img src="/img/u40.png" width="25" height="21"  style="margin-left:16px; margin-top:9px"/>  <span class="font1">卖出</span></div>
                        <div id="div6"><%=order.specification.quantity.currency%></div>
                        <div id="div7">
                            <p style="font-size:30px; font-weight:bold"><%=(order.specification.totalPrice.value*1000)/(order.specification.quantity.value*1000)%></p>
                            <p>挂单价</p>
                        </div>
                        <div id="div8"><span class="font2">成交价：<%=order.price.toFixed(2)%>众享币/份</span></div>
                        <div id="div9"><img src="/img/xx_5.png" width="1" height="126" /></div></td>
                    <td  align="center"><p class="font2">份额 <span style="font-size:24px; font-weight:bold"><%=order.specification.quantity.value%></span>份</p>
                </tr>
                <tr>
                    <td align="center" valign="middle">
                        <input name="tj" type="button" class="an1" value="撤&nbsp;&nbsp;单"onclick="tijiaodengdai1(this,<%=order.properties.sequence%>);" />
                    </td>
                </tr>
            </table>
        </div>
        <%}%>
        <% if(order.specification.direction == "buy") {%>
        <div class="div4" id="div4" style="	margin-top: 15px;"><table width="100%"  height="61px" border="0" align="center" cellpadding="0" cellspacing="0"  >
                <tr>
                    <td width="51%" rowspan="2"><div class="div5" id="div5" style="background-color:#66cccc"><img src="/img/u64.png" width="27" height="22"  style="margin-left:16px; margin-top:9px"/>  <span class="font1"> 买入</span></div>
                        <div id="div6"><%=order.specification.quantity.currency%></div>
                        <div id="div7">
                            <p style="font-size:30px; font-weight:bold"><%=(order.specification.totalPrice.value*1000)/(order.specification.quantity.value*1000)%></p>
                            <p>挂单价</p>
                        </div>
                        <div id="div8"><span class="font2">成交价：<%=order.price.toFixed(2)%>众享币/份</span></div>
                        <div id="div9"><img src="/img/xx_5.png" width="1" height="126" /></div></td>
                    <td  align="center"><p class="font2">份额 <span style="font-size:24px; font-weight:bold"> <%=order.specification.quantity.value%></span>份</p>
                    </td>
                </tr>
                <tr>
                    <td align="center" valign="middle">

                        <input name="tj" type="button" class="an1" value="撤&nbsp;&nbsp;单" onclick="tijiaodengdai1(this,<%=order.properties.sequence%>);"/>


                    </td>
                </tr>
            </table>
        </div>
        <%}%>
    </div>
    <%})%>
    <%}%><br />
    <br />

</div>
</body>
</html>